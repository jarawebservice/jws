extends layout

block content
  style
    include ../public/stylesheets/error.css
  
  if error && message
    h1= message
    h2= error.status
    pre #{error.stack}
    .error-page-wrap
      article.error-page.gradient
        hgroup
          h1 404
          h2 oops! page not found
        a.error-back(href='#', title='Back to site') back

